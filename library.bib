file = {:home/oliver/Documents/papers/1704.08331.pdf:pdf},
keywords = {a challenging problem and,convolutional neural networks,crucial role in solving,dynamic scene understanding is,enhances the overall perception,for applications,incorporating semantics and motion,it,monocular semantic motion segmentation,motion segmentation plays a,of the dynamic scene,scene understanding},
title = {{Joint Semantic and Motion Segmentation for dynamic scenes using Deep Convolutional Networks}},
url = {http://arxiv.org/abs/1704.08331},
year = {2017}
}

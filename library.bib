keywords = {Hmax,bag-of-words,color,gist,image descriptor,object recognition,scene categorization,segmentation,sift},
number = {PART 5},
pages = {312--324},
title = {{A new biologically inspired color image descriptor}},
volume = {7576 LNCS},

file = {:home/oliver/Documents/papers/bbat-wacv2016.pdf:pdf},
isbn = {9781509006410},
journal = {2016 IEEE Winter Conference on Applications of Computer Vision, WACV 2016},
title = {{Self-taught object localization with deep networks}},
year = {2016}
}

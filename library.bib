doi = {10.1109/ICCV.2011.6126554},
file = {:home/oliver/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sohn et al. - 2011 - Efficient learning of sparse, distributed, convolutional feature representations for object recognition.pdf:pdf},
isbn = {9781457711015},
issn = {15505499},

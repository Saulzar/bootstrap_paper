issn = {978-1-4673-8391-2},
journal = {2015 IEEE International Conference on Computer Vision (ICCV)},
keywords = {Atmospheric modeling,Computational modeling,Computer architecture,Feature extraction,Image recognition,ImageNet dataset,NVIDIA Tesla K40 GPU,Training,Visualization,bilinear CNN model,convolutional neural network,feature extraction,fine-grained visual recognition architecture,image descriptor,image label,local pairwise feature interaction,neural nets,orderless texture descriptor,source code,source code (software)},
pages = {1449--1457},

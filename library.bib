journal = {IEEE transactions on pattern analysis and machine intelligence},
keywords = {Algorithms,Artificial Intelligence,Data Compression,Data Compression: methods,Numerical Analysis, Computer-Assisted,Pattern Recognition, Automated,Pattern Recognition, Automated: methods,Reproducibility of Results,Sensitivity and Specificity,Signal Processing, Computer-Assisted},
month = {jul},

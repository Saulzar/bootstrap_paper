journal = {Lecture Notes in Computer Science (including subseries Lecture Notes in Artificial Intelligence and Lecture Notes in Bioinformatics)},
keywords = {Boltzmann machines,convolutional structures,image modeling,steerable filters,transformation equivariant representations,transformation invariance},
